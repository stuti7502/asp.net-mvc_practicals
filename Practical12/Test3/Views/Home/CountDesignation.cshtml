        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.DesignationNames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.count)
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Back to List", "Index")
