@model IEnumerable<Test3.Models.DesignationCount>

@{
    ViewBag.Title = "CountDesignation";
}

<h2>CountDesignation</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DesignationNames)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.count)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
